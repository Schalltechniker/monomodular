<SCRIPT name="timer()" script="decl last = getexpression(LCD, 'last_touch');
if(last<1)
{
	show(LCD, 0);
	show(loop_lcd, 0);
}
else if(last > 300)
{
	if(active > 0)
	{
		show(LCD, 1);
	}
	if(loop_active > 0)
	{
		show(loop_lcd, 1);
	}
}
setexpression(LCD, 'last_touch', last+1);
//setattribute(pn15, 'content', last);


" trigger_script="" trigger_type="1" trigger="1" osc_message="/LCD/timer" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="change_colors(OSC_ARGS)" script="decl i;
decl new_color = RGB(OSC_ARGS[0], OSC_ARGS[1], OSC_ARGS[2]);
decl name = findobject('pn0');
decl val = findobject('pv0');
for(i=0;i<32;i++)
{
	setattribute(name, 'color', new_color);
	setattribute(val, 'color', new_color);
	name = getnext(name);
	val = getnext(val);
}" trigger_script="" trigger_type="2" trigger="1" osc_message="/LCD/change_colors" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
