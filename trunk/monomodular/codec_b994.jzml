<SCRIPT name="receive_cc()" script="if(code.local_ring == 0)
{
	decl led_array, val;
	led_array = fill(0, 0, 25);
	if(cc_val>0)
	{
		val = floor(cc_val/10);
		if(fill_mode==0)
		{
			led_array[led_pos[val]] = led_colors[val>11];
		}
		else if(fill_mode==1)
		{
			decl i;
			for(i=0;i<13;i++)
			{
				led_array[led_pos[i]] = (i<=val)*led_colors[i>11];
			}
		}
	}
	setattribute(leds, 'colors', led_array);
}" trigger_script="cc_val" trigger_type="0" trigger="1" osc_message="/code/ring28/receive_cc" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
