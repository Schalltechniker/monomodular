<SCRIPT name="receive_cc()" script="if(code.local_ring == 0)
{
	decl led_array, val;
	led_array = fill(0, 0, 25);
	if(cc_val>0)
	{
		val = floor(cc_val/10);
		if(fill_mode==0)
		{
			led_array[led_pos[val]] = led_colors[val>11];
		}
		else if(fill_mode==1)
		{
			decl i;
			for(i=0;i<13;i++)
			{
				led_array[led_pos[i]] = (i<=val)*led_colors[i>11];
			}
		}
	}
	setattribute(leds, 'colors', led_array);
}" trigger_script="cc_val" trigger_type="0" trigger="1" osc_message="/code/ring14/receive_cc" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="receive_note()" script="white.value = note_val > 0;" trigger_script="note_val" trigger_type="0" trigger="1" osc_message="/code/ring14/receive_note" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="set_custom()" script="decl i;
decl led_array = fill(0, 0, 25);
decl val;
for(i=0;i<7;i++)
{
	val = 1<<i;
	led_array[led_pos[i]]=((custom[0] & val) == val)*(led_colors[0]);
}
for(i=0;i<3;i++)
{
	val = 1<<i;
	led_array[led_pos[i+7]]=((custom[1] & val) == val)*(led_colors[0]);
}
for(i=0;i<3;i++)
{
	val = 1<<(i+4);
	led_array[led_pos[i+10]]=((custom[1] & val) == val)*(led_colors[(i+10)>11]);
}
setattribute(leds, 'colors', led_array);
" trigger_script="custom" trigger_type="0" trigger="1" osc_message="/code/ring14/set_custom" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
</WINDOW>
