<SCRIPT name="sysex(MIDI_ARGS)" script="if((MIDI_ARGS[0]==0)&&(MIDI_ARGS[1]==1)&&(MIDI_ARGS[2]==97)&&(MIDI_ARGS[3]==4))
{
	decl header = MIDI_ARGS[4];
	if((header == 31)&&(code.local_ring==1))
	{
		decl message = subarray(MIDI_ARGS, 5, sizeof(MIDI_ARGS));
		//log('set_ring=', message, 0, 0);
		decl i;
		for(i=0;i<sizeof(code.rings);i++)
		{
			setexpression(code.rings[i], 'custom', {message[i*2], message[(i*2)+1]});
		}
		return;
	}
	else if(header == 32)
	{
		//log('local_ring = ', MIDI_ARGS[5], 0, 0);
		code.local_ring = MIDI_ARGS[5];
		return;
	}
	else if(header == 17)
	{	
		decl message = subarray(MIDI_ARGS, 5, 12);
		code.log('relative = ',message, 0, 0);
		decl i, j, val;
		for(i=0;i<4;i++)
		{
			for(j=0;j<7;j++)
			{
				val = 1<<j;
				setexpression(code.rings[(i*8)+j], 'relative', ((message[(i*2)+1] & val) == val));
			}
			setexpression(code.rings[(i*8)+7], 'relative', ((message[(i*2)+1] & val) == val));
		}
		return;
	}
	else if(header == 30)
	{	
		decl message = subarray(MIDI_ARGS, 5, 12);
		//log('ring_mode = ', message, 0, 0);
		decl i, j, val;
		for(i=0;i<4;i++)
		{
			for(j=0;j<7;j++)
			{
				val = 1<<j;
				setexpression(code.rings[(i*8)+j], 'fill_mode', ((message[(i*2)+1] & val) == val));
			}
			setexpression(code.rings[(i*8)+7], 'fill_mode', ((message[(i*2)+1] & val) == val));
		}
		return;
	} 
}

		
